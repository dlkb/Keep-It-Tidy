{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "2.1.2",
  "default_locale": "en",
  "description": "__MSG_extDesc__",
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
